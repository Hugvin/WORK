{
  "version": 2,
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/terms-of-service",
      "destination": "/terms-of-service.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy.html"
    },
    {
      "source": "/login",
      "destination": "/api/login"
    },
    {
      "source": "/callback",
      "destination": "/api/callback"
    },
    {
      "source": "/servers",
      "destination": "/api/servers"
    },
    {
      "source": "/api/guild/:guildId",
      "destination": "/api/guild/[guildId].js"
    },
    {
      "source": "/api/kick-bot/:guildId",
      "destination": "/api/kick-bot/[guildId].js"
    },
    {
      "source": "/api/bot-in-guild/:guildId",
      "destination": "/api/bot-in-guild/[guildId].js"
    }
  ],
  "middleware": [
    {
      "source": "/:path*",
      "destination": "middleware.js"
    }
  ]
}
